body {
    background-color: #deebff;
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
    overflow: hidden;
}

.rightPanel {
    margin-left: 1px;
    width: 280px;
}

.messagePanel
{
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    overflow-x:hidden;
    overflow-y:scroll;
}

#userlistTab li {
    display: inline-table;
    margin: 1px;
    padding: 0;
    width: 80px;
    height: 25px;
    font-size: 18px;
    text-align: center;
}

#userlistTab img {
    width: 16px;
    height: 16px;
    margin: 1px;
}

#userlistTab div[role="option"]
{
    height:25px;
    margin:0;
    padding:0;
}

.userListSpan{
    font-size:12px;
    margin-left:10px;
    vertical-align:middle;
    text-align:center;
}

.hScrollbar {
    overflow-x: hidden;
    overflow-y: auto;
}

#giftTab li {
    display: inline-table;
    height: 25px;
    width: 40px;
    font-size: 12px;    
    padding: 0;
    text-align: center;
}

#giftTab img {
    margin: 5px;
    padding: 1px;
    width: 39px;
    height: 39px;
    border: 1px,solid,black;
}

#giftPanel1 li {
    display: inline;
    margin-left: 2px;
    vertical-align: middle;
}

#giftPanel1 {
    overflow: hidden;
    font-size: 12px;
}

    #giftPanel1 td {
        color: #2846fa;
        margin: 0;
        padding: 0;
        overflow: hidden;
    }

.even {
    background-color: #deebff;
}

.odd {
    background-color: transparent;
}

.topPanel {
    height: 60px;
    font-size: 40px;
    background-color: white;
}

#MotionDiv img:hover{
    width:50px;
    height:50px;
}

#PublicMessage-Div #PrivateMessage-Div {
    word-break: break-all;
    word-wrap: break-word;
    white-space: normal;
}

#MessageStyleDiv {
    height: 25px;
    display: none;
}

#PublicMsgDiv-Btns {
    background-color: white;
    word-break: break-all;
    word-wrap: break-word;
    white-space: normal;
    position: absolute;
    top: 2px;
    right: 20px;
    z-index: 100;
    display: none;
}

#PrivateMsgDiv-Btns {
    background-color: white;
    word-break: break-all;
    word-wrap: break-word;
    white-space: normal;
    position: absolute;
    top: 2px;
    right: 20px;
    z-index: 100;
    display: none;
}


.InputMsgTxr {
    overflow-y: scroll;
    overflow-x: hidden;
    height: 100%;
    width: 100%;
}

    .InputMsgTxr IMG {
        width: 30px;
        height: 30px;
        background-color: transparent;
    }

.SendMsgBtn {
    position: absolute;
    width: 60px;
    height: 40px;
    top: 30px;
    left: 20px;
    background-color: #F6CD26;
    font-family: "华文新魏";
    font-size: large;
}

#Horn-Scroll {
    background-color: transparent;
}

#Public-Scroll {
    background-color: transparent;
}

#Private-Scroll {
    background-color: transparent;
}

#Public-Clear {
    padding: 2px;
    width: 50px;
    background-color: transparent;
}

#Private-Clear {
    padding: 2px;
    width: 50px;
    background-color: transparent;
}

#hornMsgPanel {
    background-color: white;
    height: 100px;
    border: 2px solid;
    border-color: lightblue;
    overflow-y: auto;
    top: 2px;
}

#toolbarHornMsg {
    z-index: 300;
    width: 100%;
    background-color: #a2c4f7;
    display: none;
    word-break: break-all;
    word-wrap: break-word;
    white-space: normal;
    position: absolute;
    top: 2px;
    right: 2px;
    filter: alpha(opacity=80);
}

#toolbarHornMsg p {
    margin: 0;
}

#PreviewGiftDiv {
    position:absolute;
    right:-100px;
    bottom:220px;
    width:200px;
    height:60px;
    padding-left:10px;
    BORDER-BOTTOM: 1px solid;
    BORDER-LEFT: 1px solid;
    BORDER-RIGHT: 1px solid;
    BORDER-TOP: 1px solid;
    border-color: #2846fa;
    background-color: #deebff;
    z-index: 2000;    
    float:left;
    display: none;
}

#PreviewImageDiv {
    position:absolute;
    bottom:255px;
    width:80px;
    height:80px;
    BORDER-BOTTOM: 1px solid;
    BORDER-LEFT: 1px solid;
    BORDER-RIGHT: 1px solid;
    BORDER-TOP: 1px solid;
    border-color: #2846fa;
    background-color: #deebff;
    z-index: 2000;
    display: none;
  
}

#PreviewGiftDiv img{
max-width:80px;
max-height:80px;
width:expression(document.body.clientWidth>document.getElementById("PreviewGiftDiv").scrollWidth? "80px": "auto" );
height:expression(document.body.clientHeight>document.getElementById("PreviewGiftDiv").scrollHeight? "70px": "auto" );
border:1px dashed #000;
}

#PreviewImageDiv img{
max-width:80px;
max-height:80px;
width:expression(document.body.clientWidth>document.getElementById("PreviewImageDiv").scrollWidth? "80px": "auto" );
height:expression(document.body.clientHeight>document.getElementById("PreviewImageDiv").scrollHeight? "80px": "auto" );
border:1px dashed #000;
}

.ToolWindow {
    position: absolute;
    left: 70px;
    bottom: 40px;
    BORDER-BOTTOM: 1px solid;
    BORDER-LEFT: 1px solid;
    BORDER-RIGHT: 1px solid;
    BORDER-TOP: 1px solid;
    border-color: #2846fa;
    background-color: #deebff;
    z-index: 1000;
    display: none;
    overflow-x: hidden;
    overflow-y: hidden;
}

.ToolWindow li {
    display: inline-table;
    height: 40px;
    margin-top:-20px;
    margin-left:0;
    margin-right:1px;
    padding:0;
    width: 62px;
    font-size: 11px;
    text-align:left;
}

.ChatMessageLabel{
    font-weight:bold;
    font-size:16px;
    font-family:Arial;
    color:black;
    text-decoration:none;
    font-style:normal;
}

.PageDiv div {
    top: 2px;
    width: 28px;
    cursor: pointer;
    height: 28px;
    text-align: center;
    position: relative;
    display: inline;
    margin: 2px;
}

    .PageDiv div img {
        width: 28px;
        height: 28px;
    }

.PageDiv .MotionContent .StampContent {
    position: absolute;
    top: 5px;
    overflow-x: hidden;
    overflow-y: hidden;
    display: none;
}

.pages {
    position: absolute;
    bottom: 0px;
    height: 28px;
    overflow-x: hidden;
    overflow-y: hidden;
}


.splitbutton-l {
    position: absolute;
    left: 1px;
    top: 1px;
    width: 78px;
    height: 58px;
    background-color: #f6d02a;
    font-size: 24px;
}

.splitbutton-r {
    position: absolute;
    top: 6px;
    right: 6px;
    width: 18px;
    height: 48px;
    background-color: gold;
}

/*-- Bottom Toolbar --*/

#toolbar {
    position: absolute; /* set fixed position for the bar */
    left: 0px;
    width: 100%;
    bottom: 0px;
    z-index: 9999; /* keep the bar on top  */
    height: 36px;
    background-color: #deebff;
    /* CSS3 */
    -moz-border-radius-topleft: 8px;
    -khtml-border-radius-topleft: 8px;
    -webkit-border-top-left-radius: 8px;
    -moz-border-radius-topright: 8px;
    -khtml-border-radius-topright: 8px;
    -webkit-border-top-right-radius: 8px;
    -moz-box-shadow: 0px 1px 10px #666, inset 1px 1px 0px #a4a4a4; /* inset creates a inner-shadow */
    -khtml-box-shadow: 0px 1px 10px #666;
    -webkit-box-shadow: 0px 1px 10px #666;
    /* CSS3 end */
    border-top: 1px solid #eee;
    border-left: 1px solid #eee;
    border-right: 1px solid #eee;
    border-bottom: 3px solid #7aaaf5;
}



span.showbar a { /* show button */
    padding: 5px;
    font-size: 10px;
    color: #989898;
}

.leftside {
    float: left;
}

.rightside {
    float: right;
}

