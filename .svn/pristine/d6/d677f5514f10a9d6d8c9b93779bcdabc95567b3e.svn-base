.tip {
    position: absolute; /* important */
    top: -75px;
    width: 250px;
    height: 78px;
    background: url(images/tip.png) no-repeat;
    float: left;
    display: none;
}


    .tip ul {
        padding: 22px 0 0 25px;
    }

        .tip ul li {
            display: inline;
            padding-left: 3px;
        }

            .tip ul li a {
                font-size: 18px;
                color: #989898;
            }

                .tip ul li a:hover {
                    color: #666;
                }

            .tip ul li small {
                font-size: 10px;
            }


