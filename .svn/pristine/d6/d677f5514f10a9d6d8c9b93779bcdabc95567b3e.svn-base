.ArtisticWindow {
    position: absolute;
    left: 200px;
    bottom: 40px;

    BORDER-BOTTOM: 1px solid;
    BORDER-LEFT: 1px solid;
    BORDER-RIGHT: 1px solid;
    BORDER-TOP: 1px solid;
    border-color: #2846fa;
    background-color: #deebff;
    z-index: 1000;
    display: none;
    overflow-x: hidden;
    overflow-y: hidden;
    text-align:center;
}

.ArtisticWindow ul {
  list-style-type:none;
}

.ArtisticWindow ul li {
  padding:2px 2px;
  margin: 0 15px 0px 0px;
}

.ArtisticWindow ul li a {
  font-size: 14px;
  color: black;
