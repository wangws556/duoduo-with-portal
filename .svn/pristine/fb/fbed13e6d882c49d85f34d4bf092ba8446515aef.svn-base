	font-size: 12px;
}
.zt14 {
	font-size: 14px;
}
.zt14 {
	color: #00F;
}
.zt12 {
	font-size: 12px;
	color: #00F;
}
.zt12 {
	color: #00F;
}
.zt12 {
	color: #F00;
}
.zt12 {
